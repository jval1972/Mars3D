begin
  if not Info_ResolveMobjType('Green Blood', @MT_GREENBLOOD) then
    MT_GREENBLOOD := Ord(MT_BLOOD);
  if not Info_ResolveMobjType('Blue Blood', @MT_BLUEBLOOD) then
